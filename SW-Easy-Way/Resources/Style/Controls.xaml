<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SW_Easy_Way.Resources.Style">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/SW-Easy-Way;component/Resources/Style/GroupBox.xaml"></ResourceDictionary>
        <ResourceDictionary Source="/SW-Easy-Way;component/Resources/Style/TabControl.xaml"></ResourceDictionary>
        <ResourceDictionary Source="/SW-Easy-Way;component/Resources/Style/Button.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    
    
    <Style BasedOn="{StaticResource CustomGroupBox}" TargetType="GroupBox" />
    <Style BasedOn="{StaticResource CustomTabControl}" TargetType="TabControl" />
    <Style BasedOn="{StaticResource CustomTabItem}" TargetType="TabItem" />
    
</ResourceDictionary>